<!DOCTYPE html>
<html>
  <head>
    <title>Hello,<%= profile.displayName %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <div>
    <button onclick="cb()">cancel booking</button>
    <script>
      function cb(){
          window.location.href='/cancelBookingConfirm';
      }
    </script>
  </div>
{profile:req.user, groupHasMadeBooking: true, table:resul, groupName:grp}
//[{"GROUPNAME":"moguempireee","ROOMNUMBER":"101","SLOTSTART":"10:00:00","SLOTEND":"12:00:00","DECISIONTIME":"2017-07-13T17:11:50.000Z"}]

  <div>

    <h1><%= groupName %></h1>
    <p>Welcome to viewBooking page, <%= profile.displayName %></p>
    <% if (groupHasMadeBooking == true) { %>
      <% for(var i=0;i < table.length; i++) {%>
        <h4>Room:<%= table[i].ROOMNUMBER %></h4>
        <h4> Your Timeslot </h4>
        <p style="color:red">starts at:<%=table[i].SLOTSTART %> </p>
        <p style="color:green">ends at:<%=table[i].SLOTEND %> </p>
        <h4> Booking Time: </h4>
        <p><%= table[i].DECISIONTIME %></p>
        <br>
      <% } %>
    <% } %>
    <% if (groupHasMadeBooking == true) { %>
      <button onclick="location.href='/booking?groupName=<%= groupName %>'"> book </button>
    <% } %>


    </div>

    <div>

    <button onclick="tz()">logout</button>
    <script>
    function tz(){
        window.location.href='/logout';

    }
    </script>

    <button onclick="z()">view general info</button>
    <script>
    function z(){
        window.location.href='/info';

    }
    </script>

    </div>

  </body>
</html>